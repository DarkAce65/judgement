{
  "name": "judgement-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install client/.husky",
    "eslint:fix": "eslint --fix \"*.config.{js,ts}\" \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write .eslintrc.js \"*.config.{js,ts}\" tsconfig.json index.html \"src/**/*.{ts,tsx,css,less}\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0",
    "*.{ts,tsx,css,html}": "prettier --list-different"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@reduxjs/toolkit": "^1.9.0",
    "antd": "^4.23.6",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-resize-aware": "^3.1.2",
    "react-router-dom": "^6.4.3",
    "socket.io-client": "^4.5.4",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "^0.25.3"
  }
