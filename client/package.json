{
  "name": "judgement-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install client/.husky",
    "eslint:fix": "eslint --fix \"*.config.{js,ts}\" \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write .eslintrc.js \"*.config.{js,ts}\" tsconfig.json index.html \"src/**/*.{ts,tsx,css,less}\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0",
    "*.{ts,tsx,css,html}": "prettier --list-different"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "antd": "^4.20.7",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.5",
    "@types/react": "^18.0.10",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.19",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vitest": "^0.13.1"
  }
}
