{
  "name": "judgement-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky install client/.husky",
    "eslint:fix": "eslint --fix \"*.config.js\" \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write .eslintrc.js \"*.config.js\" tsconfig.json \"public/**/*.html\" \"src/**/*.{ts,tsx,css,less}\"",
    "start": "nodemon -w craco.config.js -w ./src/antd.customize.less --exec \"craco start\"",
    "build": "craco build",
    "test": "craco test"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0",
    "*.{ts,tsx,css,html}": "prettier --list-different"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": true
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "2.3.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@craco/craco": "^6.1.2",
    "@reduxjs/toolkit": "^1.6.0",
    "antd": "^4.16.2",
    "craco-antd": "^1.19.0",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "socket.io-client": "^4.1.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2"
  }
}
